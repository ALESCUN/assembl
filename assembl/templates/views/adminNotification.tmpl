<form class="core-form mbn">
	<fieldset>
			<div class="control-group">
				<label for="<%= notification.get('@id') %>" class="control-label"></label>
				<div class="controls">
					<label class="checkbox">
						<input type="checkbox" class="js_adminNotification" value="<%= notification.get('@type') %>" <%= notification.get('status') === 'ACTIVE' ? 'checked':'' %> id="<%= notification.get('@id') %>">
						<%= i18n.gettext(notification.get('human_readable_description')) %>
					</label>
				</div>
			</div>
	</fieldset>
</form>